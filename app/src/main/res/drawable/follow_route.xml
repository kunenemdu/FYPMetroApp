<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group android:name="group">
                <group android:name="vector_1">
                    <path
                        android:name="line"
                        android:pathData="M 1 10 L 8 10"
                        android:strokeColor="#0f9d58"
                        android:strokeWidth="4"/>
                    <path
                        android:name="path"
                        android:pathData="M 9 10 L 15 10"
                        android:strokeColor="#0f9d58"
                        android:strokeWidth="4"/>
                    <path
                        android:name="path_1"
                        android:pathData="M 16 10 L 23 10"
                        android:strokeColor="#0f9d58"
                        android:strokeWidth="4"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="line">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="1"
                android:duration="999"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/decelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="1"
                android:duration="1499"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/decelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
</animated-vector>
