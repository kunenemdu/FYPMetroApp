<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="line"
                android:pathData="M 1 10 L 3 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path"
                android:pathData="M 4.5 10 L 6.5 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path_1"
                android:pathData="M 8 10 L 10 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path_2"
                android:pathData="M 11.5 10 L 13.5 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path_3"
                android:pathData="M 15 10 L 17 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path_4"
                android:pathData="M 18.5 10 L 20.5 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
            <path
                android:name="path_5"
                android:pathData="M 22 10 L 24 10"
                android:strokeColor="#0f9d58"
                android:strokeWidth="1"
                android:strokeLineCap="round"/>
        </vector>
    </aapt:attr>
    <target android:name="line">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="500"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="1000"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="1500"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="2000"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_4">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="2500"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
    <target android:name="path_5">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="3000"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"
                android:repeatMode="restart"
                android:repeatCount="infinite"/>
        </aapt:attr>
    </target>
</animated-vector>
